{
  "name": "owlbrew",
  "version": "0.0.1",
  "description": "Demo of React + Webpack + Docker",
  "author": "Niklas Nyholm <nyholmniklas@gmail.com>",
  "scripts": {
    "deploy": "webpack -p --config ./webpack.production.config.js",
    "server": "webpack-dev-server",
    "test": "eslint js/ webpack.*.js --cache && webpack --config ./webpack.config.js",
    "build": "NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --colors",
    "start": "node ./js/server.js"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.4",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-es2015": "^6.9.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "6.10.3",
    "underscore": "1.8.3",
    "webpack-dev-server": "2.2.0"
  },
  "dependencies": {
    "babel-loader": "^6.2.4",
    "webpack": "2.2.0",
    "express": "^4.13.4",
    "mcfly": "1.0.0",
    "react": "^15.3.1",
    "react-addons-update": "15.4.2",
    "react-dom": "^15.3.1",
    "react-mixin": "3.0.5",
    "semantic-ui": "^2.2.10",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.67.2"
  },
  "license": "MIT"
}
